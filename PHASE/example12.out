BEGIN COMMAND_LINE 
PHASE -S332554 example1.txt example12.out 
END COMMAND_LINE 

BEGIN OUTFILE_LIST
example12.out_freqs : haplotype frequency estimates
example12.out_pairs : most likely haplotype pairs for each individual
example12.out_recom : estimates of recombination parameters
example12.out_monitor : file for monitoring convergence
END OUTFILE_LIST

